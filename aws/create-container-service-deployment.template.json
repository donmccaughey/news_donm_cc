{
    "serviceName": "news",
    "containers": {
        "news": {
            "image": "public.ecr.aws/d2g3p0u7/news:latest",
            "command": [],
            "environment": {
                "AWS_ACCESS_KEY_ID": "{{AWS_ACCESS_KEY_ID}}",
                "AWS_SECRET_ACCESS_KEY": "{{AWS_SECRET_ACCESS_KEY}}",
                "AWS_DEFAULT_REGION": "us-west-2"
            },
            "ports": {
                "80": "HTTP"
            }
        }
    },
    "publicEndpoint": {
        "containerName": "news",
        "containerPort": 80,
        "healthCheck": {
            "healthyThreshold": 2,
            "unhealthyThreshold": 2,
            "timeoutSeconds": 2,
            "intervalSeconds": 5,
            "path": "/",
            "successCodes": "200-499"
        }
    }
}
